		task.wait(0.3)
		for i, existingNotif in pairs(activeNotifications) do
			local newPos = UDim2.new(0, 0, 1, -((NOTIFICATION_HEIGHT + NOTIFICATION_PADDING) * i))
			TweenService:Create(existingNotif, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {Position = newPos}):Play()
		end
		
		task.wait(0.3)
		if Notif then
			Notif:Destroy()
		end
	end)
	
	-- –ö–ª–∏–∫ –¥–ª—è –±—ã—Å—Ç—Ä–æ–≥–æ –∑–∞–∫—Ä—ã—Ç–∏—è
	Notif.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			-- –£–¥–∞–ª—è–µ–º –∏–∑ —Å–ø–∏—Å–∫–∞
			for i, notif in pairs(activeNotifications) do
				if notif == Notif then
					table.remove(activeNotifications, i)
					break
				end
			end
			
			-- –ê–Ω–∏–º–∞—Ü–∏—è –∑–∞–∫—Ä—ã—Ç–∏—è
			TweenService:Create(Notif, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {Size = UDim2.new(0, 0, 0, NOTIFICATION_HEIGHT)}):Play()
			task.wait(0.2)
			
			-- –û–±–Ω–æ–≤–ª—è–µ–º –ø–æ–∑–∏—Ü–∏–∏ –æ—Å—Ç–∞–ª—å–Ω—ã—Ö
			for i, existingNotif in pairs(activeNotifications) do

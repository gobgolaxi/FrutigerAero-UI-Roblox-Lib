	local function UpdateAllElementsTheme()
		-- –û–±–Ω–æ–≤–ª—è–µ–º –∫–Ω–æ–ø–∫–∏
		for _, btnData in pairs(uiElements.buttons) do
			TweenService:Create(btnData.Button, TweenInfo.new(0.3), {
				BackgroundColor3 = CurrentTheme.ElementBg,
				TextColor3 = CurrentTheme.Text
			}):Play()
			if btnData.Stroke then
				TweenService:Create(btnData.Stroke, TweenInfo.new(0.3), {
					Color = CurrentTheme.Stroke
				}):Play()
			end
		end
		
		-- –û–±–Ω–æ–≤–ª—è–µ–º —Ç–æ–≥–≥–ª—ã
		for _, toggleData in pairs(uiElements.toggles) do
			TweenService:Create(toggleData.Frame, TweenInfo.new(0.3), {
				BackgroundColor3 = CurrentTheme.ElementBg
			}):Play()
			TweenService:Create(toggleData.Label, TweenInfo.new(0.3), {
				TextColor3 = CurrentTheme.Text
			}):Play()
			TweenService:Create(toggleData.CheckBox, TweenInfo.new(0.3), {
				BackgroundColor3 = CurrentTheme.CheckBoxBg
			}):Play()
			if toggleData.Indicator then
				TweenService:Create(toggleData.Indicator, TweenInfo.new(0.3), {
					BackgroundColor3 = CurrentTheme.Accent
				}):Play()
			end
		end
		
		-- –û–±–Ω–æ–≤–ª—è–µ–º —Å–ª–∞–π–¥–µ—Ä—ã
		for _, sliderData in pairs(uiElements.sliders) do
			TweenService:Create(sliderData.Frame, TweenInfo.new(0.3), {
				BackgroundColor3 = CurrentTheme.ElementBg
			}):Play()
			TweenService:Create(sliderData.Label, TweenInfo.new(0.3), {
				TextColor3 = CurrentTheme.Text
			}):Play()
			TweenService:Create(sliderData.ValueLabel, TweenInfo.new(0.3), {
				TextColor3 = CurrentTheme.Accent
			}):Play()
			TweenService:Create(sliderData.Fill, TweenInfo.new(0.3), {
				BackgroundColor3 = CurrentTheme.Accent
			}):Play()
		end
		
		-- –û–±–Ω–æ–≤–ª—è–µ–º —Ç–µ–∫—Å—Ç–æ–≤—ã–µ –ø–æ–ª—è
		for _, textData in pairs(uiElements.textInputs) do

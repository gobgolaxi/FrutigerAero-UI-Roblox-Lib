			}):Play()
			TweenService:Create(textData.SubmitBtn, TweenInfo.new(0.3), {
				BackgroundColor3 = CurrentTheme.Accent
			}):Play()
		end
		
		-- –û–±–Ω–æ–≤–ª—è–µ–º ListSetting
		for _, listData in pairs(uiElements.listSettings) do
			TweenService:Create(listData.Frame, TweenInfo.new(0.3), {
				BackgroundColor3 = CurrentTheme.ElementBg
			}):Play()
			TweenService:Create(listData.Label, TweenInfo.new(0.3), {
